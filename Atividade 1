3)

void main() {
 
  var area = "Área total do trapézio é:";
  double base_maior = 5;
  double base_menor = 20;
  double altura = 40;
  
  double total_trapezio = base_maior+base_menor*altura/2;
 
 
  print ("-Trapézio-");
  print ("Base Maior = 5");
  print ("Base Menor = 20");
  print ("Altura = 40");
  print ("Calculo: Base maior + Base menor x altura / 2");
  print (area + " " + "$total_trapezio");
 }

