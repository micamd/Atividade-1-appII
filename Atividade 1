4)

void main() {
 
  var area = "Área total do quadrado é: ";
  double base = 20;
  double altura = 40;
  
  double area_quadrado = base*altura;
  
  print ("-Quadrado-");
  print ("Base = " + "$base");
  print ("Altura = " + "$altura");
  print ("Calculo: Base x Altura");
  print (area + "" + "$area_quadrado");
 
 
 }

